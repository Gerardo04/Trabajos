<%include _header%>

    <h1>Registro Equip</h1>

    <form action="/Requip" method="post">
        <div class="form-group">
            <input name="description" type="text" class="form-control" placeholder="Descripcion" required autofocus/>
        </div>
        <div class="form-group">
            <input name="defense" type="text" class="form-group" placeholder="Defensa" required/>
        </div>
        <div class="form-group">
            <input name="category" type="text" class="form-group" placeholder="Categoria" required/>
        </div>
        <div class="form-group">
            <input name="weight" type="text" class="form-group" placeholder="Weight" required/>
        </div>
        <input type="submit" value=" Registrar" class="btn btn-primary btn-block" />
    </form>

    <%include _footer%>