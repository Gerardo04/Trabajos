<%include _header%>
    <h1>Aqui esta tu equipamiento</h1>

    <%equipment.forEach((equip) =>{%>
        <div class="panel panel-default">
            <div class="panel-heading">
                <a href="/equipment/<%=equip.description%>">
                    <%=equip.name()%>
                </a>
            </div>
            <%if (equip.defense){%>
                <div class="panel-body">
                    <%=equip.defense%>
                </div>
                <%}%>
        </div>
        <%})%>

            <%include _footer%>